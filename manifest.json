{
  "manifest_version": 2,
  "name": "KodeKloud Course Downloader",
  "version": "0.2",
  "browser_action": {
    "default_icon": "logo.png"
  },
  "content_scripts": [
    {
      "matches": ["https://kodekloud.com/courses/*"],
      "js": ["jquery-3.4.1.min.js", "content.js"]
    }
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "permissions": ["downloads"]
}
